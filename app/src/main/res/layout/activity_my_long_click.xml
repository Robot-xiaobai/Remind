<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.administrator.mapleone.activity.MyLongClickActivity">

    <Button
        android:id="@+id/bt_long_click_up"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/up_my"/>

    <Button
        android:id="@+id/bt_long_click_down"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/down_my"/>

    <Button
        android:id="@+id/bt_click_jump"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/bt_click_jump"/>

    <Button
        android:id="@+id/bt_isalive"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/bt_islive"/>
</LinearLayout>
